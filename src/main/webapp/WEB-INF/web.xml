<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
         version="2.5">
  <servlet>
    <servlet-name>HelloAppEngine</servlet-name>
    <servlet-class>fr.unice.polytech.sacc.tp1.HelloAppEngine</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloAppEngine</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Articles</servlet-name>
    <servlet-class>fr.unice.polytech.sacc.tp1.ArticlesEngine</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Articles</servlet-name>
    <url-pattern>/articles</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Scaling</servlet-name>
    <servlet-class>fr.unice.polytech.sacc.tp1.ScalingEngine</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Scaling</servlet-name>
    <url-pattern>/scaling</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Push</servlet-name>
    <servlet-class>fr.unice.polytech.sacc.tp1.PushEngine</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Push</servlet-name>
    <url-pattern>/push</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DataStore</servlet-name>
    <servlet-class>fr.unice.polytech.sacc.tp1.DataStoreEngine</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DataStore</servlet-name>
    <url-pattern>/datastore</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
